  "main": "server.mjs",
  "scripts": {
    "build:client": "cd ../ssk-avenues-js && npm run build",
    "build:server": "rm -rf build && webpack --mode production --config webpack.config.js",
    "build": "npm run build:client && npm run build:server",
    "start:dev": "nodemon server.mjs",
    "start:prod:safe": "pm2 startup ubuntu",
    "start:prod": "npm run build && pm2 start ecosystem.config.js --env production",
    "stop:prod": "pm2 stop ecosystem.config.js",
    "start": "npm run start:prod:safe && npm run start:prod"
  },
